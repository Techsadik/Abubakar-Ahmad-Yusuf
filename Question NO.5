// Function to return the sum of two integers
fun sum(a: Int, b: Int): Int {
    return a + b
}

fun main() {
    // Call the function with different values
    val result1 = sum(5, 10)
    val result2 = sum(20, 30)
    val result3 = sum(-3, 7)

    // Display the results
    println("Sum of 5 and 10 is: $result1")
    println("Sum of 20 and 30 is: $result2")
    println("Sum of -3 and 7 is: $result3")
}
